{
    "name": "adrhumphreys/solverstroper",
    "description": "Hyperbole and a Half",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe"
    ],
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-cms": "4.2.x-dev",
        "symbiote/silverstripe-gridfieldextensions": "^3.2",
        "wilr/silverstripe-googlesitemaps": "^2.1",
        "bummzack/sortablefile": "^2.1",
        "jonom/focuspoint": "^3.0",
        "undefinedoffset/sortablegridfield": "^2.0"
    },
    "config": {
        "process-timeout": 600,
        "platform": {
            "php": "7.0.15"
        }
    },
    "autoload": {
        "psr-4": {
            "Keppler\\Site\\": "app/src/"
        },
        "classmap": [
            "app/src/pages/Page.php",
            "app/src/controllers/PageController.php"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
